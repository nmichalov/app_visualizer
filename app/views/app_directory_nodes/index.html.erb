<h1>Listing app_directory_nodes</h1>

<table>
  <tr>
    <th>Parent node</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_directory_nodes.each do |app_directory_node| %>
  <tr>
    <td><%= app_directory_node.parent_node_id %></td>
    <td><%= app_directory_node.name %></td>
    <td><%= link_to 'Show', app_directory_node %></td>
    <td><%= link_to 'Edit', edit_app_directory_node_path(app_directory_node) %></td>
    <td><%= link_to 'Destroy', app_directory_node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

