<h1>Listing app_files</h1>

<table>
  <tr>
    <th>App directory</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_files.each do |app_file| %>
  <tr>
    <td><%= app_file.app_directory_id %></td>
    <td><%= app_file.name %></td>
    <td><%= link_to 'Show', app_file %></td>
    <td><%= link_to 'Edit', edit_app_file_path(app_file) %></td>
    <td><%= link_to 'Destroy', app_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App file', new_app_file_path %>
